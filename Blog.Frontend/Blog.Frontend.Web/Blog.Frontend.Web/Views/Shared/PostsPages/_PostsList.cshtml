@model List<Blog.Backend.Services.BlogService.Contracts.BlogObjects.Post>

<div class="dashboard-content">
    <ul>
        @if (Model.Count > 0)
        {
            foreach (var item in Model)
            {
            <li>
                @Html.Partial("_UserPost", GetPostView(item))
            </li>
            }
        }
        else
        {
            <li>
                <span>There are no posts</span>
            </li>
        }
    </ul>
</div>

@functions
{
    public Blog.Frontend.Web.Models.PostView GetPostView(Blog.Backend.Services.BlogService.Contracts.BlogObjects.Post post)
    {
        var postview = new Blog.Frontend.Web.Models.PostView { Post = post, IsAdd = false, IsEdit = false, DisplayComments = false };
        return postview;
    }
}